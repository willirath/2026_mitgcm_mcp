! when mix_scheme is 'TOY'.

&param_tke
tke_c_k        = 0.1
tke_c_eps      = 0.7
tke_alpha      = 30.0
tke_mxl_min    = 1.0e-8
tke_mxl_choice = 2
tke_kappaM_min = 0.0
tke_kappaM_max = 100.0
tke_cd         = 3.75
tke_surf_min   = 1.0e-4
tke_min        = 1.0e-6
